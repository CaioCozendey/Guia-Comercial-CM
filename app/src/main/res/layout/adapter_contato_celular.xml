<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/view"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="288dp"
        android:layout_height="48dp"
        android:background="@color/azulClaro"
        app:layout_constraintCircleRadius="32dp"/>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/numero"
        app:layout_constraintTop_toTopOf="@id/view"
        app:layout_constraintEnd_toEndOf="@id/view"
        app:layout_constraintStart_toStartOf="@id/view"
        app:layout_constraintBottom_toBottomOf="@id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/_22_99856_2641"
        android:textColor="@color/white"
        android:fontFamily="@font/roboto_regular"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/circle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/icone"
        app:layout_constraintBottom_toBottomOf="@id/icone"
        app:layout_constraintStart_toStartOf="@id/icone"
        app:layout_constraintEnd_toEndOf="@id/icone"
        android:src="@drawable/ic_circle"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/icone"
        app:layout_constraintTop_toTopOf="@id/view"
        app:layout_constraintStart_toStartOf="@id/view"
        app:layout_constraintBottom_toBottomOf="@id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_telephone"
        android:layout_marginStart="16dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>